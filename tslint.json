{
    "extends": ["tslint:recommended", "tslint-config-prettier"],
    "rules": {
        "no-internal-module": true,
        "no-trailing-whitespace": true,
        "no-null-keyword": true,
        "prefer-const": true,
        "jsdoc-format": true,
        "curly": [
            false
        ],
        "object-literal-shorthand": false,
        "interface-name": [
            false
        ],
        "array-type": false
    }
}